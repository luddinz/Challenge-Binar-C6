<form method="post" action="/admin/edit/<%= ugame.id %>?_method=PUT">
  <div class="mb-3">
    <label for="username" class="form-label">Username</label>
    <input
      type="text"
      class="form-control"
      name="username"
      id="username"
      value="<%= ugame.username %>"
      required
    />
  </div>

  <div class="mb-3">
    <label for="email" class="form-label">Email</label>
    <input
      type="email"
      class="form-control"
      id="email"
      name="email"
      value="<%= ugame.email %>"
      required
    />
  </div>

  <div class="mb-3">
    <label for="password" class="form-label">Password</label>
    <input
      type="password"
      class="form-control"
      id="password"
      name="password"
      value="<%= ugame.password %>"
      required
    />
  </div>

  <div class="mb-3">
    <label for="firstname" class="form-label">First Name</label>
    <input
      type="text"
      class="form-control"
      name="firstName"
      id="firstname"
      value="<%= ugame.ugamebio.firstName %>"
      required
    />
  </div>

  <div class="mb-3">
    <label for="lastName" class="form-label">Last Name</label>
    <input
      type="text"
      class="form-control"
      name="lastName"
      id="lastName"
      value="<%= ugame.ugamebio.lastName %>"
      required
    />
  </div>

  <div class="mb-3">
    <label for="phone" class="form-label">Phone Number</label>
    <input
      type="tel"
      class="form-control"
      name="phoneNumber"
      id="firstname"
      value="<%= ugame.ugamebio.phoneNumber %>"
      required
    />
  </div>

  <div class="row">
    <div class="col-auto">
      <button type="button" onclick="history.back()" class="btn btn-danger">
        Batal
      </button>
    </div>

    <div class="col-auto">
      <button type="reset" class="btn btn-secondary">Reset</button>
    </div>

    <div class="col-auto">
      <button type="submit" class="btn btn-primary">Submit</button>
    </div>
  </div>
</form>
